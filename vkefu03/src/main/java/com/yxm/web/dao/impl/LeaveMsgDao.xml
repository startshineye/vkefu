<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yxm.web.dao.LeaveMsgDao">
	   <insert id="save">
		insert into
			fouss_im_leavemsg(ID,USERID,USERNAME,USERINFO,EMAIL,MOBILE,QQ,MESSAGE,AGENTID,STARTTIME,HANDLETIME,STATUS,HANDLEDESCRIPTION)
		values(LEAVEMSG_SEQ.Nextval,#{userId},#{userName},#{userInfo},#{email},#{mobile},#{qq},#{message},#{agentId},#{startTime},#{handleTime},#{status},#{handleDescription})
		</insert>
</mapper>